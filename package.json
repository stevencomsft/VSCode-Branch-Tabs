{
  "name": "branch-tabs",
  "displayName": "Branch Tabs",
  "description": "Keeps track of tabs across branch sessions",
  "license": "MIT",
  "repository": "https://garage-09.visualstudio.com/_git/VS%20Code%20Branch%20Tabs%20Extension",
  "version": "0.0.8",
  "publisher": "stevencoMsft",
  "icon": "assets/BranchTabsIcon.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionDependencies": [
    "vscode.git",
    "vscode.git-base"
  ],
  "contributes": {
    "views": {
      "scm": [
        {
          "id": "branchTabsView",
          "name": "Branch Tabs"
        }
      ]
    },
    "commands": [
      {
        "command": "branchTabs.clearAll",
        "title": "Branch Tabs - Clear All Saved Tabs"
      },
      {
        "command": "branchTabs.disableAutoRestore",
        "title": "Branch Tabs - Disable Auto Restore"
      },
      {
        "command": "branchTabs.enableAutoRestore",
        "title": "Branch Tabs - Enable Auto Restore"
      }
    ]
  },
  "main": "./dist/extension.js",
  "extensionKind": [
    "workspace"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "build:vsix": "vsce package",
    "build:publish": "vsce publish",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}
